<div class="container">
<h1>My movie lists</h1>
  <div class="cards my-4">
    <% @lists.each do |list| %>
      <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url(<%= cl_image_path list.photo.key %>)">
        <%= list.name %>
        <%= link_to '', list_path(list), class: 'card-link' %> 
      </div>
    <% end %>
  </div>
</div>
